<config xmlns="http://tail-f.com/ns/config/1.0">
    <pm xmlns="urn:rdns:com:ericsson:oammodel:ericsson-pm">
        <job>
            <name>bsf_job</name>
            <granularity-period>fifteen-min</granularity-period>
            <requested-job-state>active</requested-job-state>
            <type>measurement-job</type>
            <measurement-reader>
                <name>nbsf_management_reader</name>
                <group-ref>nbsf_management</group-ref>
            </measurement-reader>
			<measurement-reader>
                <name>bsf_system_metrics_reader</name>
                <group-ref>bsf_system_metrics</group-ref>
            </measurement-reader>
			<measurement-reader>
                <name>bsf_nrf_reader</name>
                <group-ref>bsf_nrf</group-ref>
            </measurement-reader>
			<measurement-reader>
                <name>diameter_ingress_reader</name>
                <group-ref>diameter_ingress</group-ref>
            </measurement-reader>
			<measurement-reader>
                <name>diameter_egress_reader</name>
                <group-ref>diameter_egress</group-ref>
            </measurement-reader>
			<measurement-reader>
                <name>diameter_kpis_reader</name>
                <group-ref>diameter_kpis</group-ref>
            </measurement-reader>
			<measurement-reader>
                <name>unsuccessful_binding_database_operations_group_reader</name>
                <group-ref>unsuccessful_binding_database_operations_group</group-ref>
            </measurement-reader>
			<measurement-reader>
                <name>bsf_database_client_reader</name>
                <group-ref>bsf_database_client</group-ref>
            </measurement-reader>
			<measurement-reader>
                <name>bsf_database_server_reader</name>
                <group-ref>bsf_database_server</group-ref>
            </measurement-reader>
   			<measurement-reader>
                <name>bsf_service_reader</name>
                <group-ref>bsf_service</group-ref>
            </measurement-reader>
            <measurement-reader>
                <name>bsf_traffic_kpis_reader</name>
                <group-ref>bsf_traffic_kpis</group-ref>
            </measurement-reader>
            <measurement-reader>
                <name>bsf_system_kpis_reader</name>
                <group-ref>bsf_system_kpis</group-ref>
            </measurement-reader>
            <measurement-reader>
                <name>bsf_probe_virtual_tap_broker_reader</name>
                <group-ref>bsf_probe_virtual_tap_broker</group-ref>
            </measurement-reader>
            <measurement-reader>
                <name>bsf_vtap_reader</name>
                <group-ref>bsf_vtap</group-ref>
            </measurement-reader>
        </job>
		<job>
            <name>unsuccessful_binding_database_operations_job</name>
            <granularity-period>one-min</granularity-period>
            <requested-job-state>active</requested-job-state>
            <type>threshold-job</type>
            <measurement-reader>
                <name>unsuccessful_binding_database_operations_reader</name>
                <measurement-type-ref>unsuccessful_binding_database_operations</measurement-type-ref>
				<group-ref>unsuccessful_binding_database_operations_group</group-ref>
				<threshold-direction>increasing</threshold-direction>
				<threshold-monitoring>
					<name>unsuccessful_binding_database_operations_threshold_monitoring</name>
					<threshold-high>5</threshold-high>
					<threshold-low>1</threshold-low>
					<threshold-severity>major</threshold-severity>
				</threshold-monitoring>
            </measurement-reader>
        </job>
        <job>
            <name>bsf_pvtb_diff_recv_send_successes_packets_ratio_job</name>
            <granularity-period>one-min</granularity-period>
            <requested-job-state>active</requested-job-state>
            <type>threshold-job</type>
            <measurement-reader>
                <name>bsf_pvtb_diff_recv_send_successes_packets_ratio_reader</name>
                <measurement-type-ref>bsf_pvtb_diff_recv_send_successes_packets_ratio</measurement-type-ref>
                <group-ref>bsf_probe_virtual_tap_broker</group-ref>
                <threshold-direction>increasing</threshold-direction>
                <threshold-monitoring>
                    <name>bsf_pvtb_diff_recv_send_successes_packets_ratio_threshold_monitoring</name>
                    <threshold-high>5</threshold-high>
                    <threshold-low>0</threshold-low>
                    <threshold-severity>major</threshold-severity>
                </threshold-monitoring>
            </measurement-reader>
        </job>
        <job>
            <name>bsf_tapcollector_out_tap_frames_dropped_ratio_job</name>
            <granularity-period>one-min</granularity-period>
            <requested-job-state>active</requested-job-state>
            <type>threshold-job</type>
            <measurement-reader>
                <name>bsf_tapcollector_out_tap_frames_dropped_ratio_reader</name>
                <measurement-type-ref>bsf_tapcollector_out_tap_frames_dropped_ratio</measurement-type-ref>
                <group-ref>bsf_vtap</group-ref>
                <threshold-direction>increasing</threshold-direction>
                <threshold-monitoring>
                    <name>bsf_tapcollector_out_tap_frames_dropped_ratio_threshold_monitoring</name>
                    <threshold-high>5</threshold-high>
                    <threshold-low>0</threshold-low>
                    <threshold-severity>major</threshold-severity>
                </threshold-monitoring>
            </measurement-reader>
        </job>
        <job>
            <name>bsf_vtap_segments_dropped_ratio_job</name>
            <granularity-period>one-min</granularity-period>
            <requested-job-state>active</requested-job-state>
            <type>threshold-job</type>
            <measurement-reader>
                <name>bsf_vtap_segments_dropped_ratio_reader</name>
                <measurement-type-ref>bsf_vtap_segments_dropped_ratio</measurement-type-ref>
                <group-ref>bsf_vtap</group-ref>
                <threshold-direction>increasing</threshold-direction>
                <threshold-monitoring>
                    <name>bsf_vtap_segments_dropped_ratio_threshold_monitoring</name>
                    <threshold-high>5</threshold-high>
                    <threshold-low>0</threshold-low>
                    <threshold-severity>major</threshold-severity>
                </threshold-monitoring>
            </measurement-reader>
        </job>
		    <job>
            <name>database_server_storage_utilization_job</name>
            <granularity-period>one-min</granularity-period>
            <requested-job-state>active</requested-job-state>
            <type>threshold-job</type>
            <measurement-reader>
                <name>database_server_storage_utilization_reader</name>
                <measurement-type-ref>database_server_storage_utilization</measurement-type-ref>
				<group-ref>bsf_database_server</group-ref>
				<threshold-direction>increasing</threshold-direction>
				<threshold-monitoring>
					<name>database_server_storage_utilization_threshold_monitoring1</name>
					<threshold-high>30</threshold-high>
					<threshold-low>25</threshold-low>
					<threshold-severity>minor</threshold-severity>
				</threshold-monitoring>
				<threshold-monitoring>
					<name>database_server_storage_utilization_threshold_monitoring2</name>
					<threshold-high>40</threshold-high>
					<threshold-low>35</threshold-low>
					<threshold-severity>major</threshold-severity>
				</threshold-monitoring>
				<threshold-monitoring>
					<name>database_server_storage_utilization_threshold_monitoring3</name>
					<threshold-high>50</threshold-high>
					<threshold-low>45</threshold-low>
					<threshold-severity>critical</threshold-severity>
				</threshold-monitoring>
            </measurement-reader>
        </job>
        <job>
            <name>binding_database_node_unavailable_job</name>
            <granularity-period>one-min</granularity-period>
            <requested-job-state>active</requested-job-state>
            <type>threshold-job</type>
            <measurement-reader>
                <name>binding_database_node_unavailable_reader</name>
                <measurement-type-ref>binding_database_node_unavailable</measurement-type-ref>
                <group-ref>bsf_database_server</group-ref>
                <threshold-direction>increasing</threshold-direction>
                <threshold-monitoring>
                    <name>binding_database_node_unavailable_threshold_monitoring</name>
                    <threshold-high>1</threshold-high>
                    <threshold-low>0</threshold-low>
                    <threshold-severity>critical</threshold-severity>
                </threshold-monitoring>
            </measurement-reader>
        </job>
        <job>
            <name>binding_database_datacenter_unavailable_job</name>
            <granularity-period>one-min</granularity-period>
            <requested-job-state>active</requested-job-state>
            <type>threshold-job</type>
            <measurement-reader>
                <name>binding_database_datacenter_unavailable_reader</name>
                <measurement-type-ref>binding_database_datacenter_unavailable</measurement-type-ref>
                <group-ref>bsf_database_server</group-ref>
                <threshold-direction>increasing</threshold-direction>
                <threshold-monitoring>
                    <name>binding_database_datacenter_unavailable_threshold_monitoring</name>
                    <threshold-high>1</threshold-high>
                    <threshold-low>0</threshold-low>
                    <threshold-severity>critical</threshold-severity>
                </threshold-monitoring>
            </measurement-reader>
        </job>
    </pm>
</config>
