                <additional-text>The NRF-group is unavailable</additional-text>
                <configured-severity/>
                <default-severity>major</default-severity>
            </alarm-type>
            <alarm-type>
                <id>SeppBadConfiguration</id>
                <major-type>193</major-type>
                <minor-type>12124165</minor-type>
                <specific-problem>SEPP, Bad Configuration</specific-problem>
                <event-type>processing-error-alarm</event-type>
                <probable-cause/>
