                <additional-text>The configuration provided is faulty</additional-text>
                <configured-severity/>
                <default-severity>major</default-severity>
            </alarm-type>
            <alarm-type>
                <id>SeppUnresolvableHosts</id>
                <major-type>193</major-type>
                <minor-type>12124166</minor-type>
