<config xmlns="http://tail-f.com/ns/config/1.0">
    <fm xmlns="urn:rdns:com:ericsson:oammodel:ericsson-fm">
		<alarm-model>
            <id>eric-sepp-fm</id>
			<alarm-type>
                <id>MissingCertificate</id>
                <major-type>193</major-type>
                <minor-type>12124161</minor-type>
                <specific-problem>SEPP, Certificate Missing</specific-problem>
                <event-type>other</event-type>
                <probable-cause/>
                <is-stateful>true</is-stateful>
                <additional-text>An asymmetric key or/and a certificate authority are unavailable which can result in the failure of the secure connection that uses them.</additional-text>
                <configured-severity/>
                <default-severity>critical</default-severity>
            </alarm-type>
			<alarm-type>
                <id>SeppNfServiceInstanceNotReachable</id>
                <major-type>193</major-type>
                <minor-type>12124162</minor-type>
                <specific-problem>SEPP, NF Service Instance Not Reachable</specific-problem>
                <event-type>communications-alarm</event-type>
                <probable-cause/>
                <is-stateful>true</is-stateful>
                <additional-text>The NF Service Instance is not reachable for service requests</additional-text>
                <configured-severity/>
                <default-severity>major</default-severity>
            </alarm-type>
			<alarm-type>
                <id>SeppHighLoad</id>
                <major-type>193</major-type>
                <minor-type>12124163</minor-type>
                <specific-problem>SEPP, High Load</specific-problem>
                <event-type>other</event-type>
                <probable-cause>207</probable-cause>
                <is-stateful>true</is-stateful>
