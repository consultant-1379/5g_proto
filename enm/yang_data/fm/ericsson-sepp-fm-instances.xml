                <configured-severity/>
                <default-severity>major</default-severity>
            </alarm-type>
            <alarm-type>
                <id>SeppHighNumberOfIngressTcpConnections</id>
                <major-type>193</major-type>
                <minor-type>12124167</minor-type>
                <specific-problem>SEPP, High number of open incoming TCP connections</specific-problem>
                <event-type>communications-alarm</event-type>
