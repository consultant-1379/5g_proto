                <event-type>communications-alarm</event-type>
                <probable-cause/>
                <is-stateful>false</is-stateful>
                <additional-text>Invalid discovered NF instance not updated in database</additional-text>
                <configured-severity/>
                <default-severity>major</default-severity>
            </alarm-type>
            <alarm-type>
                <id>SeppNfDiscoveryDataUpdateError</id>
                <major-type>193</major-type>
                <minor-type>12124172</minor-type>
                <specific-problem>SEPP, NF discovery data update error</specific-problem>
                <event-type>communications-alarm</event-type>
                <probable-cause/>
                <is-stateful>false</is-stateful>
                <additional-text>Unable to update discovered data in database</additional-text>
                <configured-severity/>
                <default-severity>major</default-severity>
            </alarm-type>
            <alarm-type>
                <id>SeppNfInstanceUnavailable</id>
                <major-type>193</major-type>
                <minor-type>12124173</minor-type>
                <specific-problem>SEPP, NF Instance Unavailable</specific-problem>
                <event-type>communications-alarm</event-type>
                <probable-cause/>
                <is-stateful>true</is-stateful>
                <additional-text>The NF Failures have exceeded the configured threshold</additional-text>
                <configured-severity/>
                <default-severity>major</default-severity>
            </alarm-type>
            <alarm-type>
                <id>SeppSecurityCapabilityNegotiationFailure</id>
                <major-type>193</major-type>
                <minor-type>12124174</minor-type>
