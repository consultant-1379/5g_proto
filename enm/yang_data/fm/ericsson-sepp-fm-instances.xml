            </alarm-type>
            <alarm-type>
                <id>SeppMessagesRejectedByGlobalRateLimiting</id>
                <major-type>193</major-type>
                <minor-type>12124169</minor-type>
                <specific-problem>SEPP, Messages Rejected by Global Rate Limiting</specific-problem>
                <event-type>communications-alarm</event-type>
                <is-stateful>true</is-stateful>
                <additional-text>The percentage of rejected messages forwarded from SEPP to RLF has exceeded the predefined limit</additional-text>
                <configured-severity/>
                <default-severity>warning</default-severity>
            </alarm-type>
            <alarm-type>
                <id>SeppEnvoySegmentsDroppedRatio</id>
