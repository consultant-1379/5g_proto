                        <in-request-screening-case-ref>ingress_req_from_all_RP</in-request-screening-case-ref>
                        <routing-case-ref>routing_from_external</routing-case-ref>
                        <roaming-partner>
                            <name>RP_BE</name>
                            <comment>Trusted roaming partner in PLMN 206-33 (RP-BE)</comment>
                            <in-request-screening-case-ref>ingress_req_from_RP_BE</in-request-screening-case-ref>
                            <routing-case-ref>routing_from_external</routing-case-ref>
