                            <trusted-certificate-list>sc-traf-root-ca-list1</trusted-certificate-list>
                            <supports-target-apiroot>true</supports-target-apiroot>
                        </roaming-partner>
                        <roaming-partner>
                            <name>RP_SE</name>
                            <comment>Trusted roaming partner in PLMN 240-60 (RP-SE)</comment>
                            <routing-case-ref>routing_from_external</routing-case-ref>
                            <domain-name>*.region1.amf.5gc.mnc060.mcc240.3gppnetwork.org</domain-name>
                            <trusted-certificate-list>sc-traf-root-ca-list1</trusted-certificate-list>
                            <supports-target-apiroot>true</supports-target-apiroot>
                        </roaming-partner>
                    </external-network>
                    <failover-profile>
                        <name>fob1</name>
                        <request-time-budget>3456</request-time-budget>
                        <target-timeout>2345</target-timeout>
                        <preferred-host-retries-max>3</preferred-host-retries-max>
                        <target-nf-pool-reselects-max>1</target-nf-pool-reselects-max>
                        <last-resort-nf-pool-reselects-max>1</last-resort-nf-pool-reselects-max>
                        <retry-condition>
                            <http-status>500</http-status>
                            <http-status>501</http-status>
                            <http-status>502</http-status>
                            <http-status>503</http-status>
                            <http-status>504</http-status>
                            <reset>false</reset>
                            <connect-failure>false</connect-failure>
                            <refused-stream>false</refused-stream>
                        </retry-condition>
                    </failover-profile>
                    <ingress-connection-profile>
                        <name>default_ingress_connection_profile</name>
                        <max-connection-duration>10</max-connection-duration>
                        <hpack-table-size>0</hpack-table-size>
                    </ingress-connection-profile>
                    <message-data>
                        <name>3GppRegistration</name>
                        <extractor-regex>/nudm-uecm/v1/imsi-\d+/registrations/(?P&lt;reg&gt;[\w\-]+)$</extractor-regex>
                        <path/>
                    </message-data>
                    <message-data>
                        <name>imsi_mcc_mnc</name>
                        <extractor-regex>/nudm-uecm/v1/imsi-(?P&lt;mcc&gt;\d\d\d)(?P&lt;mnc&gt;\d\d\d).*/registrations</extractor-regex>
                        <path/>
                    </message-data>
                    <message-data>
                        <name>mcc_mnc_data</name>
                        <extractor-regex>(https?://)?.*5gc\.mnc(?P&lt;mnc&gt;\d+)\.mcc(?P&lt;mcc&gt;\d+)</extractor-regex>
                        <header>3gpp-Sbi-target-apiRoot</header>
                    </message-data>
                    <message-data>
                        <name>nf_data</name>
                        <extractor-regex>(https?://)?.*\.(?P&lt;nf&gt;\w+)\.5gc\.mnc</extractor-regex>
                        <header>3gpp-Sbi-target-apiRoot</header>
                    </message-data>
                    <nf-pool>
                        <name>NfUdm_pref</name>
                        <own-network-ref>ownNetworkDE</own-network-ref>
                        <nf-pool-discovery>
                            <name>NfUdm_pref_static</name>
                            <static-nf-instance-data-ref>NfUdm_pref_static</static-nf-instance-data-ref>
                            <update-interval>1h</update-interval>
                        </nf-pool-discovery>
                    </nf-pool>
                    <nf-pool>
                        <name>sepp_RP_BE_pool</name>
                        <roaming-partner-ref>RP_BE</roaming-partner-ref>
                        <nf-pool-discovery>
                            <name>sepp_RP_BE_pool_discovery</name>
                            <static-nf-instance-data-ref>static_sepp_RP_BE_data</static-nf-instance-data-ref>
                            <update-interval>1h</update-interval>
                        </nf-pool-discovery>
                    </nf-pool>
                    <nf-pool>
                        <name>sepp_RP_SE_pool</name>
                        <roaming-partner-ref>RP_SE</roaming-partner-ref>
                        <nf-pool-discovery>
                            <name>sepp_RP_SE_pool_discovery</name>
                            <static-nf-instance-data-ref>static_sepp_RP_SE_data</static-nf-instance-data-ref>
                            <update-interval>1h</update-interval>
                        </nf-pool-discovery>
                    </nf-pool>
                    <own-network>
                        <name>ownNetworkDE</name>
                        <service-address-ref>vpn_ownInternal</service-address-ref>
                        <routing-case-ref>routing_from_internal</routing-case-ref>
                        <trusted-certificate-list>sc-traf-root-ca-list1</trusted-certificate-list>
                    </own-network>
                    <request-screening-case>
                        <name>ingress_req_from_RP_BE</name>
                        <message-data-ref>3GppRegistration</message-data-ref>
                        <screening-rule>
                            <name>apply_also_global_screening</name>
                            <condition/>
                            <screening-action>
                                <name>continue_with_global_screening</name>
                                <action-go-to>
                                    <request-screening-case-ref>ingress_req_from_all_RP</request-screening-case-ref>
                                </action-go-to>
                            </screening-action>
                        </screening-rule>
                        <screening-rule>
                            <name>category1</name>
                            <condition>var.reg==&apos;amf-non-3gpp-access&apos;</condition>
                            <screening-action>
                                <name>reject_cat1_message</name>
                                <action-reject-message>
                                    <status>503</status>
                                    <title>Service Unavailable</title>
                                    <detail>amf-non-3gpp-access not allowed for RP_BE</detail>
                                    <format>json</format>
                                </action-reject-message>
                            </screening-action>
                        </screening-rule>
                    </request-screening-case>
                    <request-screening-case>
                        <name>ingress_req_from_all_RP</name>
                        <message-data-ref>imsi_mcc_mnc</message-data-ref>
                        <screening-rule>
                            <name>category2</name>
                            <condition>not (var.mcc==&apos;262&apos; and var.mnc==&apos;073&apos;)</condition>
                            <screening-action>
                                <name>reject_cat2_message</name>
                                <action-reject-message>
                                    <status>503</status>
                                    <title>Service Unavailable</title>
                                    <detail>Registration only allowed for Home PLMN IMSI series</detail>
                                    <format>json</format>
                                </action-reject-message>
                            </screening-action>
                        </screening-rule>
                    </request-screening-case>
                    <routing-case>
                        <name>routing_from_external</name>
                        <message-data-ref>mcc_mnc_data</message-data-ref>
                        <message-data-ref>nf_data</message-data-ref>
                        <routing-rule>
                            <name>psepp_to_nf_stricttarget</name>
                            <condition>var.mcc==&apos;262&apos; and var.mnc==&apos;073&apos; and var.nf==&apos;udm&apos;</condition>
                            <routing-action>
                                <name>action-route-strict</name>
                                <action-route-preferred>
                                    <failover-profile-ref>fob1</failover-profile-ref>
                                    <from-target-api-root-header/>
                                    <target-nf-pool>
                                        <nf-pool-ref>NfUdm_pref</nf-pool-ref>
                                    </target-nf-pool>
                                </action-route-preferred>
                            </routing-action>
                        </routing-rule>
                    </routing-case>
                    <routing-case>
                        <name>routing_from_internal</name>
                        <message-data-ref>mcc_mnc_data</message-data-ref>
                        <routing-rule>
                            <name>internal_to_RP_BE_csepp_rr</name>
                            <condition>var.mcc==&apos;206&apos; and var.mnc==&apos;033&apos;</condition>
                            <routing-action>
                                <name>action-route-round-robin</name>
                                <action-route-round-robin>
                                    <failover-profile-ref>fob1</failover-profile-ref>
                                    <target-roaming-partner>
                                        <roaming-partner-ref>RP_BE</roaming-partner-ref>
                                    </target-roaming-partner>
                                </action-route-round-robin>
                            </routing-action>
                        </routing-rule>
                        <routing-rule>
                            <name>internal_to_RP_SE_csepp_rr</name>
                            <condition>var.mcc==&apos;240&apos; and var.mnc==&apos;060&apos;</condition>
                            <routing-action>
                                <name>action-route-round-robin</name>
                                <action-route-round-robin>
                                    <failover-profile-ref>fob1</failover-profile-ref>
                                    <target-roaming-partner>
                                        <roaming-partner-ref>RP_SE</roaming-partner-ref>
                                    </target-roaming-partner>
                                </action-route-round-robin>
                            </routing-action>
                        </routing-rule>
                    </routing-case>
                    <service-address>
                        <name>vpn_ownExternal</name>
                        <fqdn>sepp1.region1.sepp.5gc.mnc073.mcc262.3gppnetwork.org</fqdn>
                        <tls-port>32067</tls-port>
                        <ipv4-address>10.63.143.71</ipv4-address>
                        <asymmetric-key-ref>asymKey</asymmetric-key-ref>
                    </service-address>
                    <service-address>
                        <name>vpn_ownInternal</name>
                        <fqdn>sepp1.region1.sepp.5gc.mnc073.mcc262.3gppnetwork.org</fqdn>
                        <port>30711</port>
                        <ipv4-address>10.63.143.71</ipv4-address>
                    </service-address>
                    <static-nf-instance-data>
                        <name>NfUdm_pref_static</name>
                        <static-nf-instance>
                            <name>nf-seppsim-p1</name>
                            <nf-type>udm</nf-type>
                            <scp-domain/>
                            <static-nf-service>
                                <name>nudm-uecm</name>
                                <capacity>10</capacity>
                                <priority>10</priority>
                                <set-id/>
                                <address>
                                    <scheme>http</scheme>
                                    <fqdn>nfudm1.region1.udm.5gc.mnc073.mcc262.3gppnetwork.org</fqdn>
                                </address>
                            </static-nf-service>
                        </static-nf-instance>
                        <static-nf-instance>
                            <name>nf-seppsim-p2</name>
                            <nf-type>udm</nf-type>
                            <scp-domain/>
                            <static-nf-service>
                                <name>nudm-uecm</name>
                                <capacity>10</capacity>
                                <priority>10</priority>
                                <set-id/>
                                <address>
                                    <scheme>http</scheme>
                                    <fqdn>nfudm2.region1.udm.5gc.mnc073.mcc262.3gppnetwork.org</fqdn>
                                </address>
                            </static-nf-service>
                        </static-nf-instance>
                    </static-nf-instance-data>
                    <static-nf-instance-data>
                        <name>static_sepp_RP_BE_data</name>
                        <static-nf-instance>
                            <name>sepp2_BE-seppsim-p7</name>
                            <nf-type>sepp</nf-type>
                            <nf-set-id>nf-set_1</nf-set-id>
                            <scp-domain/>
                            <static-nf-service>
                                <name>nsepp-telescopic</name>
                                <capacity>10</capacity>
                                <priority>10</priority>
                                <set-id/>
                                <address>
                                    <scheme>http</scheme>
                                    <fqdn>sepp-be.region1.sepp.5gc.mnc033.mcc206.3gppnetwork.org</fqdn>
                                </address>
                            </static-nf-service>
                        </static-nf-instance>
                    </static-nf-instance-data>
                    <static-nf-instance-data>
                        <name>static_sepp_RP_SE_data</name>
                        <static-nf-instance>
                            <name>sepp_SE-seppsim-p8</name>
                            <nf-type>sepp</nf-type>
                            <nf-set-id>nf-set_1</nf-set-id>
                            <scp-domain/>
                            <static-nf-service>
                                <name>nsepp-telescopic</name>
                                <capacity>10</capacity>
                                <priority>10</priority>
                                <set-id/>
                                <address>
                                    <scheme>http</scheme>
                                    <fqdn>sepp-se.region1.sepp.5gc.mnc060.mcc240.3gppnetwork.org</fqdn>
                                </address>
                            </static-nf-service>
                        </static-nf-instance>
                    </static-nf-instance-data>
                </nf-instance>
            </sepp-function>
        </config>
    </edit-config>
</rpc>
]]>]]>

